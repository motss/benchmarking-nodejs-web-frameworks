{
  "name": "test-api",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "keywords": [
    "npm",
    "test-api",
    "zumata"
  ],
  "homepage": "",
  "license": "UNLICENSED",
  "author": "Thenpm sta Zumata Team",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "build": "sh ./run-build.sh production",
    "build:clean": "sh ./run-build.sh clean production",
    "build:test": "rm -rf dist && babel src/ -d dist/",
    "check": "npm run lint && npm run nsp && npm run snyk",
    "debug": "node --inspect src/index.js",
    "debug:esm": "node --inspect -r @std/esm src/index.js",
    "lint": "eslint ./src",
    "nsp": "nsp check",
    "snyk": "snyk test",
    "start": "env-cmd ./.env node -r @std/esm src/index.js",
    "watch": "babel -w src/ -d dist/"
  },
  "dependencies": {
    "@std/esm": "^0.9.2",
    "env-cmd": "^5.1.0",
    "express": "^4.15.4",
    "fastify": "^0.27.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-babili": "^0.1.3",
    "babel-preset-env": "^1.5.2",
    "eslint": "^4.1.0",
    "eslint-config-zumata": "^0.6.0",
    "nsp": "^2.6.3",
    "snyk": "^1.34.4"
  },
  "engines": {
    "node": ">= 6.9.1",
    "npm": ">= 3.10.8"
  },
  "@std/esm": {
    "esm": "all",
    "cjs": true,
    "await": true,
    "gz": true
  }
}
